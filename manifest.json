{
  "manifest_version": 2,
  "name": "Markdown Copy",
  "description": "A Chrome Extension to copy page links and images in Markdown format"
  "version": "0.1",
  "icons": {
    "16": "icon.png",
    "24": "icon.png",
    "32": "icon.png"
  },
  "browser_action": {
    "default_title": "Markdown Copy"
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "contextMenus"
    "notifications"
  ]
} 
